{
  "name": "@feugene/browser-logger",
  "version": "0.0.5",
  "description": "Do logging into browser",
  "keywords": [
    "log",
    "logging",
    "console"
  ],
  "license": "MIT",
  "homepage": "https://github.com/efureev/browser-logger",
  "scripts": {
    "build": "yarn build.ts && yarn build.browser",
    "build.ts": "rm -rf dist && tsc -p src",
    "build.browser": "npx webpack",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "coverage": "nyc npm run test && nyc report --reporter=html"
  },
  "main": "dist/index.js",
  "browser": "browser/logger.min.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "browser/logger.min.js",
    "browser/logger.min.js.map"
  ],
  "engines": {
    "node": ">= 16"
  },
  "devDependencies": {
    "@types/expect": "24.3.0",
    "@types/mocha": "9.1.0",
    "coveralls": "^3.1.1",
    "mocha": "9.2.1",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/efureev/browser-logger.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not ie 11",
    "not op_mini all"
  ]
}
